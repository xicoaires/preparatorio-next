programa
{
	
	funcao inicio()
	{
		real a, b, c, media

		escreva("Digite a primeira nota: ")
		leia(a)
		escreva("Digite a segunda nota: ")
		leia(b)
		escreva("Digite a tercera nota: ")
		leia(c)
		media = ((a * 2) + (b * 3) + (c * 5)) / 10

		escreva("MÉDIA = ", media)
	}
}
